{
    "multipart": [
		{ "apply": { "model": "mineminenomi:block/kairosekibars_post_ends" }},
		
        { "when": { "north": false, "east": false, "south": false, "west": false }, "apply": { "model": "mineminenomi:block/kairosekibars_post" } },
        { "when": { "north": true, "east": false, "south": false, "west": false }, "apply": { "model": "mineminenomi:block/kairosekibars_cap" } },
        { "when": { "north": false, "east": true, "south": false, "west": false }, "apply": { "model": "mineminenomi:block/kairosekibars_cap", "y": 90 } },
        { "when": { "north": false, "east": false, "south": true, "west": false }, "apply": { "model": "mineminenomi:block/kairosekibars_cap_alt" } },
        { "when": { "north": false, "east": false, "south": false, "west": true }, "apply": { "model": "mineminenomi:block/kairosekibars_cap_alt", "y": 90 } },
		
		{ "when": { "north": true }, "apply": { "model": "mineminenomi:block/kairosekibars_side" } },
        { "when": { "east": true }, "apply": { "model": "mineminenomi:block/kairosekibars_side", "y": 90 } },
        { "when": { "south": true }, "apply": { "model": "mineminenomi:block/kairosekibars_side_alt" } },
        { "when": { "west": true }, "apply": { "model": "mineminenomi:block/kairosekibars_side_alt", "y": 90 } }
    ]
}
